CREATE TABLE workspace_permission (
    id BIGINT(20) UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY,
    user_id BIGINT(20) UNSIGNED NOT NULL COMMENT '同步pre_user表',
    is_create TINYINT(1) NOT NULL,
    workspace_id VARCHAR(100) DEFAULT NULL COMMENT '传入工作区的slug',
    create_time DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP,
    update_time DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP
);

CREATE TABLE history_aichat (
    id BIGINT(20) UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY,
    session_id BIGINT(20) UNSIGNED NOT NULL,
    model VARCHAR(100) DEFAULT NULL,
    user_id VARCHAR(100) DEFAULT NULL COMMENT '传入工作区的slug',
    last_messages VARCHAR(100) NOT NULL,
    create_time DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP,
    update_time DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP
);
